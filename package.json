  "name": "reactjs-app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "webpack --progress -p --config config/webpack.${NODE_ENV}.config.js --bail",
    "build:safe": "concurrently --kill-others-on-fail \"npm:test:check-coverage\" \"npm:build\" \"npm:lint\" \"npm:stylelint\"",
    "profile": "webpack --progress -p --config config/webpack.${NODE_ENV}.config.js --profile --json > profile.json",
    "start:server": "webpack-dev-server --mode development --config config/webpack.${NODE_ENV}.config.js",
    "start": "concurrently \"npm:start:server\" \"npm:test:watch\" \"npm:lint:test:watch\"",
    "stylelint": "stylelint --syntax=scss --max-warnings=0 --fix 'src/**/*.scss'",
    "lint": "eslint --max-warnings=0 src/ test/",
    "lint:test:watch": "esw --fix --changed --watch test",
    "lint:fix": "npm run lint -- --fix",
    "test": "export NODE_ENV=test && export NODE_PATH=$NODE_PATH:/app/src && mocha --require @babel/register --recursive --require test/setup.js --require @babel/polyfill --exit",
    "test:coverage": "nyc npm run test",
    "test:check-coverage": "nyc --check-coverage npm run test",
    "test:watch": "export NODE_PATH=$NODE_PATH:/app/src && babel-node test/watch.js"
  },
  "keywords": [
    "React",
    "Javascript",
    "Node",
    "Redux"
  ],
